<% case params[:calculation][:type] %>
<% when 'anchors' %>
  <%= turbo_stream.update "anchors-result" do %>
    <%= render 'anchor_result' %>
  <% end %>

<% when 'user_capacity' %>
  <%= turbo_stream.update "capacity-result" do %>
    <%= render 'capacity_result' %>
  <% end %>

<% when 'slide_runout' %>
  <%= turbo_stream.update "runout-result" do %>
    <%= render 'runout_result' %>
  <% end %>

<% when 'wall_height' %>
  <%= turbo_stream.update "wall-height-result" do %>
    <%= render 'wall_height_result' %>
  <% end %>

<% end %>
