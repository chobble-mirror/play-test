<%= render 'form/fieldset', form: form, i18n_base: i18n_base, legend_key: 'structure_dimensions' do %>
  <%= render 'form/number', field: :stitch_length, min: 0 %>
  <%= render 'form/number', field: :evacuation_time, min: 0 %>
  <%= render 'form/number', field: :unit_pressure, min: 0 %>
  <%= render 'form/number', field: :blower_tube_length, min: 0 %>
  <%= render 'form/number', field: :step_size, min: 0 %>
  <%= render 'form/number', field: :critical_fall_off_height, min: 0 %>
  <%= render 'form/number', field: :trough_depth, min: 0 %>
  <%= render 'form/number', field: :trough_width, min: 0 %>
<% end %>